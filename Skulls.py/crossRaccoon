#-------

u=zip(b,a)
k=[]
counter=0
for i,j in u:
    if i==j:
	print "x"	
        #print i,'--',j
    else:
        k.append(j)
        #print i,'  ',j

#print '\n', y

print '\n Beaver Differences = ',len(k)

